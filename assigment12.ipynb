    "num = int(input(\"write a number\"))\n",
    "\n",
    "count = 0\n",
    "\n",
    "for i in range(1, num+1):\n",
    "    if not (num % i) :\n",
    "        count += 1\n",
    "if (num == 0) or (num == 1) or (count >= 3) :\n",
    "    print(num, \"is not a prime number\")\n",
    "else : print(num, \"is a prime number\")    \n",
    "    \n",
    "\n"
